import org.junit.jupiter.api.Assertions;
import org.junit.jupiter.api.Test;
import java.util.Arrays;
import java.util.List;

class MyTestClass {
    private static String sqlFormatedList(List<String> stakeholderVzids) {
        StringBuilder sb = new StringBuilder();
        sb.append("('");
        for (String i : stakeholderVzids) {
            sb.append(i + "', '");
        }
        sb.deleteCharAt(sb.length() - 1);
        sb.deleteCharAt(sb.lastIndexOf(","));
        sb.append(")");
        return sb.toString();
    }

    @Test
    void testSqlFormattedList() {
        List<String> stakeholderVzids = Arrays.asList("abc", "def", "ghi");
        String expected = "('abc', 'def', 'ghi')";

        String result = sqlFormatedList(stakeholderVzids);

        Assertions.assertEquals(expected, result);
    }
}
