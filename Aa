import org.junit.jupiter.api.Assertions;
import org.junit.jupiter.api.Test;
import org.mockito.InjectMocks;
import org.mockito.Mock;
import org.mockito.Mockito;
import org.mockito.MockitoAnnotations;
import org.springframework.jdbc.core.JdbcTemplate;
import org.springframework.jdbc.support.rowset.SqlRowSet;

class YourClassNameTest {
    @Mock
    private JdbcTemplate jdbcTemplate;

    @InjectMocks
    private YourClassName yourClass;

    @BeforeEach
    void setUp() {
        MockitoAnnotations.openMocks(this);
    }

    @Test
    void testGetInServiceDate() {
        // Mock the behavior of the JdbcTemplate queryForRowSet method
        String expectedDate = "2023-06-07";
        SqlRowSet mockRowSet = Mockito.mock(SqlRowSet.class);
        Mockito.when(jdbcTemplate.queryForRowSet(Mockito.anyString())).thenReturn(mockRowSet);
        Mockito.when(mockRowSet.next()).thenReturn(true);
        Mockito.when(mockRowSet.getString(Mockito.anyString())).thenReturn(expectedDate);

        // Call the method and assert the result
        String result = yourClass.getInServiceDate("pscJobId");
        Assertions.assertEquals(expectedDate, result);
    }
}
